{
  "functions": {
    "app/api/chat/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/chat",
      "headers": [
        {
          "key": "Connection",
          "value": "keep-alive"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-transform"
        }
      ]
    }
  ]
}
